Map_RolAsesor:
	Mapping
		LOAD 
			ASESOR_ID, 
			ROL
		FROM
		[lib://COMP_Stage (eficacia_admin_analitica)/SGI/Colombia/SEVILLA_SA/IPV_Asesor.qvd]
    	(qvd);

